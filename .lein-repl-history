(def f {:a {:b 1 :c 2} :d {:e 3 :g 4}})
(clojure.walk/prewalk-demo f)
(clojure.walk/postwalk-demo f)
(clojure.walk/postwalk println f)
(clojure.walk/prewalk println f)
(clojure.walk/prewalk #(do (println %) %) f)
(clojure.walk/prewalk #(do (if (number? %) (println %)) %) f)
(clojure.walk/postwalk #(do (if (number? %) (println %)) %) f)
(clojure.walk/postwalk #(if (number? %) %) f)
(clojure.walk/postwalk #(if (number? %) (+ 4 %) %) f)
(exit)
lein run
